on init
    # LiveDisplay mediatek
    chown system system /sys/devices/platform/mtk_disp_mgr.0/rgb
    chmod 0660 /sys/devices/platform/mtk_disp_mgr.0/rgb

service vendor.livedisplay-hal-2-0-karnak /vendor/bin/hw/vendor.lineage.livedisplay@2.0-service-karnak
    class late_start
    user system
    group system
